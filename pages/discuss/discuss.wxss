 page{
  background-color: #F5F5F5;
 }
 page .pin {
  width: 750rpx;
  background: #ffffff;
  margin-top: 30rpx;
}
page .pin .All {
  margin: 0 30rpx;
}
page .pin .All .bar {
  width: 690rpx;
  height: 68rpx;
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
  line-height: 108rpx;
  box-shadow: 0px 1px 0px 0px #eeeeee;
  margin-bottom: 45rpx;
  padding-bottom: 36rpx;
}
page .pin .All .All-item {
  height: 437rpx;
  margin-bottom: 47rpx;
  box-shadow: 0px 1px 0px 0px #eeeeee;
}
page .pin .All .All-item .userInfo {
  height: 100rpx;
  width: 690rpx;
  position: relative;

}
page .pin .All .All-item .userInfo .userInfo-tou {
  width: 88rpx;
  height: 88rpx;

  border-radius: 50%;
}
page .pin .All .All-item .userInfo .userInfo-all {
  width: 228px;
  position: absolute;
  height: 80rpx;
  top: -7rpx;
  left: 121rpx;
  
}
page .pin .All .All-item .userInfo .userInfo-all .userInfo-name {
  width: 160rpx;
  height: 46rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
  line-height: 26px;
}
page .pin .All .All-item .userInfo .userInfo-all .userInfo-pinx {
  position: absolute;
  left: 0;
  top: 49rpx;
  height: 46rpx;

}
page .pin .All .All-item .userInfo .userInfo-all .userInfo-pinx .userInfo-pin {
  width: 20rpx;
  height: 20rpx;
  margin-right: 12rpx;
  padding: 5rpx 0;

  
}
page .pin .All .All-item .userInfo-pinx .van-icon  {
  color: #C17372;
  font-size: 25rpx;
}

page .pin .All .All-item .userInfo .tui {
  width: 710rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #777777;
  line-height: 45rpx;
  margin-top: -33rpx;
  margin-bottom: 33rpx;
  
  
}

page .pin .All .All-item .userInfo .up {
  display: flex;
  width: 690rpx;
  height:150rpx;
  align-items: center;
}
page .pin .All .All-item .userInfo .up view {
  display: flex;
  width: 160rpx;
  height:120rpx;
  align-items: center;
  margin: 0 20rpx;
  /* justify-content: space-around; */
}
page .pin .All .All-item .userInfo .up image {
width:100%;
height:100%;

}
