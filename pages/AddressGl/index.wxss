page{
  background: #F5F5F5;
}
.list {
  width: 750rpx;
  background: white;
  margin-bottom: 15rpx;
}
.list .content{
  width: 690rpx;
  height: 200rpx;
  margin: 0 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.list .content .left{
   width: 30%;

}
.list .content .left .name{

  margin: 20rpx 0;
}
.list .content .left .btns{
background-color: #C28E90;
width: 100rpx;
height: 35rpx;
display: flex;
align-items: center;
justify-content: center;
color: white;
border-radius: 10rpx;
}
.list .content .right{
  width: 60%;

}

.list .content .right .ph{

  margin: 20rpx 0;
}
.list .content .right .site{
width: 85%;
overflow: hidden;
white-space: nowrap;
text-overflow: ellipsis

}
.list .content .goEit{
  width: 5%;
 height: 200rpx;
 display: flex;
 justify-content: center;
 align-items: center;
}
.list .content .goEit view{
width: 40rpx;
height: 40rpx;
 display: flex;
 justify-content: center;
 align-items: center;
}
.list .content .goEit image{
  width: 35rpx;
  height: 35rpx;
}
.box{
  display: flex;
  justify-content: center;
  align-items: center;
  height: 200rpx;
}
.addbtn{
  width: 690rpx;
  height: 80rpx;
  background:rgba(61,54,94,1);
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0 auto;
  margin-bottom: 50rpx;
  border-radius: 10rpx;
  color: white;
  margin-top: 50rpx;
}