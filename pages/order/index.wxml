<view>
	<view class="tabs">
		<van-tabs color="#C28E90" active="{{ 0 }}" swipeable>
			<van-tab title="全部">
				<view class="order" wx:for="{{All}}" wx:key="index">
					<view class="title f28">
						订单编号：{{item.orderSn}}
						<view style="color:#C28E90 ;width: 63px;" wx:if="{{item.status==1?true:false}}">已支付</view>
						<view style="color:#C28E90 ;width: 63px;" wx:if="{{item.status==0?true:false}}">待付款</view>
						<view style="color:#C28E90 ;width: 63px;" wx:if="{{item.status==2?true:false}}">已发货</view>
						<view style="color:#C28E90 ;width: 63px;" wx:if="{{item.status==3?true:false}}">已收货</view>
						<view style="color:#C28E90 ;width: 63px;" wx:if="{{item.status==4?true:false}}">退货中</view>
						<view style="color:#C28E90 ;width: 63px;" wx:if="{{item.status==5?true:false}}">退货成功</view>
						<view style="color:#C28E90 ;width: 63px;" wx:if="{{item.status==6?true:false}}">已完成</view>
						<view style="color:#C28E90 ;width: 63px;" wx:if="{{item.status==7?true:false}}">退款成功</view>
					</view>
					<view class="conter">
						<view class="aaa">
							<image src="{{item.goodsPic}}" Alt="{{item.goodsPic}}"></image>
						</view>
						<view style=" flex: auto;  position: relative;">
							<view class="f32">{{item.goodsName}}</view>
							<view class="f26"> {{item.goodsAttr}}</view>
							<view class="f32 bottom">
								￥{{item.goodsPrice}}<text>*{{item.goodsQuantity}}</text></view>
						</view>
					</view>
					<view class="amout">
						共{{item.goodsQuantity}}件
						<view>应付总金额：
							<text>￥{{item.goodsQuantity*item.goodsPrice}}</text>
						</view>
					</view>
					<view class="btns">
						<view class="payOne" hover-class="useBg">取消支付</view>
						<view class="pay" bindtap="goShop" hover-class="useBg">继续支付</view>
					</view>
				</view>
